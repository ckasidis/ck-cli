{
  "name": "ck-cli",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "format": "prettier --write '**/*.{html,css,scss,js,jsx,ts,tsx,md,mdx,json,yml,yaml,gql,graphql}'",
    "commit": "cz",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.1.3",
    "commander": "^10.0.1",
    "commitizen": "^4.3.0",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "typescript": "^5.0.4"
  }
}
